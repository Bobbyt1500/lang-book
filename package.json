{
  "name": "language-book",
  "displayName": "language-book",
  "description": "",
  "version": "1.0.1",
  "repository": {
    "type": "git",
    "url": "https://github.com/Bobbyt1500/lang-book.git"
  },
  "engines": {
    "vscode": "^1.66.0"
  },
  "categories": [
    "Other"
  ],
  "activationEvents": [
    "onRenderer:language-book-renderer",
    "onNotebook:language-book"
  ],
  "main": "./dist/extension.js",
  "contributes": {
    "notebookRenderer": [
      {
        "id": "language-book-renderer",
        "displayName": "Language Notebook",
        "entrypoint": "./dist/renderer/index.js",
        "mimeTypes": [
          "x-application/language-book-data"
        ],
        "requiresMessaging": "always"
      }
    ],
    "notebooks": [
      {
        "id": "language-book",
        "type": "language-book",
        "displayName": "Language Book",
        "selector": [
          {
            "filenamePattern": "*.langbook"
          }
        ]
      }
    ],
    "languages": [
      {
        "id": "langbook-syntax",
        "aliases": [
          "Langbook Syntax"
        ]
      }
    ]
  },
  "scripts": {
    "vscode:prepublish": "npm run package",
    "compile": "webpack",
    "watch": "webpack --watch",
    "package": "webpack --mode production --devtool hidden-source-map",
    "compile-tests": "tsc -p . --outDir out",
    "watch-tests": "tsc -p . -w --outDir out",
    "pretest": "npm run compile-tests && npm run compile && npm run lint",
    "lint": "eslint src --ext ts",
    "test": "node ./out/test/runTest.js"
  },
  "devDependencies": {
    "@types/glob": "^7.2.0",
    "@types/mocha": "^9.1.0",
    "@types/node": "14.x",
    "@types/uuid": "^8.3.4",
    "@types/vscode": "^1.66.0",
    "@typescript-eslint/eslint-plugin": "^5.16.0",
    "@typescript-eslint/parser": "^5.16.0",
    "@vscode/test-electron": "^2.1.3",
    "eslint": "^8.11.0",
    "glob": "^7.2.0",
    "mocha": "^9.2.2",
    "ts-loader": "^9.2.8",
    "typescript": "^4.5.5",
    "uuid": "^8.3.2",
    "webpack": "^5.70.0",
    "webpack-cli": "^4.9.2"
  },
  "dependencies": {
    "@types/vscode-notebook-renderer": "^1.60.0",
    "css-loader": "^6.7.1",
    "preact": "^10.7.1",
    "style-loader": "^3.3.1"
  }
}
